from turtle import *

# Set the speed of the turtle
speed(100)

# Set the background color
bgcolor('black')

# List of colors to cycle through
colors = ['cyan', 'magenta', 'yellow', 'green', 'blue', 'orange']

# Draw a pattern
for i in range(290):
    color(colors[i % len(colors)])  # Change color every few rotations
    circle(i)  # Draw a circle with radius i
    left(5)    # Turn the turtle 5 degrees to the left

done()
